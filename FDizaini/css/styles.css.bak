            body, html {
                margin: 0;
                padding: 0;
                width: 100%;
                height: 100%;
                background-color: black;
            }

            #main-container {
                margin: 0;
                padding: 5px 100px;
                background-color: black;
                color: white;
            }

            #main-table {
			font-family: Fantasy;
                width: 100%;
                border-collapse: collapse;
                background-color: black;
            }

            #main-table .avatar-cell {
                background-size: 48px !important;
                background-position: center !important;
                height: 48px;
                width: 48px;
                padding: 1px;
                cursor: pointer;
            }
			
            #main-table .copy-text-button {
			font-family: Fantasy;
                display: inline-block;
                border-radius: 4px;
                background-color: green;
                border: none;
                color: red;
                text-align: center;
                padding: 5px;
                width: 50px;
                transition: all 0.5s;
                cursor: pointer;
                margin: 5px;
            }

            #main-table .copy-text-button span {
			font-family: Fantasy;
                cursor: pointer;
                display: inline-block;
                position: relative;
                transition: 0.5s;
            }
            #main-table .listing-row.visited {
			    font-family: Fantasy;
                background-color: red;
                color: black;
            }
            a:link {
            color: rgb(255, 255, 255);
            background-color: transparent;
            text-decoration: none;
            }

            a:visited {
            color: rgb(0, 150, 255);
            background-color: transparent;
            text-decoration: none;
            }

            a:hover {
            color: rgb(255, 0, 0);
            background-color: transparent;
            text-decoration: underline;
            }

            a:active {
            color: yellow;
            background-color: transparent;
            text-decoration: underline;
            }
